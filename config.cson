"*":
  "autocomplete-python":
    useKite: false
    useSnippets: "all"
  core:
    restorePreviousWindowsOnStart: "no"
    telemetryConsent: "limited"
    themes: [
      "atom-material-ui"
      "atom-material-syntax"
    ]
  editor:
    fontFamily: "monaco"
    fontSize: 16
    showIndentGuide: true
    showLineNumbers: true
    tabLength: 4
  "exception-reporting":
    userId: "a399bfe3-7f2b-4535-9edc-d9312a3f6413"
  "linter-flake8":
    maxLineLength: 100
  "linter-ui-default":
    showPanel: true
    showProviderName: true
  welcome:
    showOnStartup: false
